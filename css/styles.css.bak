@import url(http://fonts.googleapis.com/css?family=Ubuntu:400,500,700,300italic&subset=latin,cyrillic-ext);

/* browser reset */

html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
abbr, address, cite, code, del, dfn, em, img, ins, kbd, q, samp,
small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li,
fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, figcaption, figure,
footer, header, hgroup, menu, nav, section, summary,
time, mark, audio, video {
    margin: 0;
    padding: 0;
    border: 0;
    outline: 0;
    font-size: 100%;
    background: transparent;
}

body {
    line-height: 1;
}

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section {
    display: block;
}

nav ul {
    list-style: none;
}

blockquote, q {
    quotes: none;
}

blockquote: before, blockquote: after,
q: before, q: after {
    content: '';
    content: none;
}

a {
    margin: 0;
    padding: 0;
    font-size: 100%;
    background: transparent;
}

/* change colours to suit your needs */

ins {
    background-color: #ff9;
    color: #000;
    text-decoration: none;
}

mark {
    background-color: #ff9;
    color: #000;
    font-style: italic;
    font-weight: bold;
}

del {
    text-decoration: line-through;
}

abbr[title], dfn[title] {
    border-bottom: 1px dotted;
    cursor: help;
}

table {
    border-collapse: collapse;
    border-spacing: 0;
}

/* change border colour to suit your needs */

hr {
    display: block;
    height: 1px;
    border: 0;
    border-top: 1px solid #cccccc;
    margin: 1em 0;
    padding: 0;
}

input, select {
    vertical-align: middle;
}

/* floating */

.left {
    float: left !important;
}

.right {
    float: right !important;
}

/* text aligments */

.text-left {
    text-align: left !important;
}

.text-right {
    text-align: right !important;
}

.text-center {
    text-align: center !important;
}

a * {
    cursor: pointer;
}

img {
    vertical-align: top;
}

* {
    outline: none;
}

/* go structure */
html {
    height: 100%;
}

body {
    background: #f8f8f8;
    font-family: 'arial';
    color: #7d7d7d;
    height: 100%;
    min-width: 996px;
}

/* typo */
h1 {
    font: 26px 'Ubuntu', Arial;
    line-height: 26px;
    //font: 700 18px/18px 'Ubuntu', sans-serif;
    color: #000;
    padding: 0 0 31px;
}

h2 {
	font: 18px 'Ubuntu', Arial;
	//font: 500 24px/27px 'Ubuntu', sans-serif;
	color: #000;
	padding: 14px 0 8px;
    //padding: 27px 0 18px;
}

h3 {
    font: 16px 'Ubuntu', Arial;
    //font: 500 18px/27px 'Ubuntu', sans-serif;
    color: #000;
    padding: 11px 0 6px;
	//padding: 2px 0 7px;
}

p {
    font-size: 12px;
    line-height: 18px;
    color: #7d7d7d;
    padding: 0 0 18px;
}

p.error {
    padding: 2px 0 0;
    font-size: 12px;
    font-style: italic;
    line-height: normal;
    color: #ccc;
}

p.note {
    font-weight:bold;
    line-height: 20px;
    font-size: 15px;
    
    color: #7d7d7d;
    padding: 0 0 41px;
    margin-top: -10px;
}

.index p.note {
    padding-bottom: 30px;
}

ul {
    list-style: none;
    padding: 0 0 9px;
}

ul li {
    padding: 0 0 9px 37px;
    font-size: 12px;
    line-height: 18px;
    color: #7d7d7d;
    background: url("../images/li-sep.gif") 22px 9px no-repeat;
}

a {
    color: #0071ac;
    text-decoration: underline;
}

a:hover {
    text-decoration: none;
}

.hr {
    height: 51px;
    background: url("../images/hr-top.png") no-repeat;
}

/* form */
/* custom-select */

.custom-select {
    position: relative;
    width: 358px;
    overflow: hidden;
}

.custom-select .output .btn {
    background: url("../images/btn-select.png") no-repeat;
    width: 36px;
    height: 38px;
    position: absolute;
    top: -2px;
    right: -5px;
}

.custom-select .output .text {
    width: 320px;
    position: absolute;
    top: 0;
    line-height: 28px;
}

.custom-select .select {
    position: relative;
    overflow: hidden;
}

.custom-select .select select {
    width: 100%;
    height: 28px;
    position: relative;
    border: none;
    background: none;
    filter: alpha(opacity=0);
    opacity: 0;
}

.custom-select select {
    border: none;
    box-shadow: none;
    background-color: transparent;
    background-image: none;
    -webkit-appearance: none;
    -moz-appearance: none;
    appearance: none;
    position: absolute;
    font: 12px 'arial';
    color: #7d7d7d;
}

.custom-select select:focus {
    outline: none;
}

input[type="text"],
input[type="password"],
textarea,
.custom-select .text {
    height: 27px;
    width: 348px;
    padding: 0 5px;
    background: #fff url("../images/input-bg.gif") repeat-x;
    border: 1px solid #e1e1e1;
    border-radius: 0;
    -webkit-border-radius: 0;
    font: 12px 'arial';
    line-height: 27px\9;
    color: #7d7d7d;
    display: inline-block;
    *display: inline;
    *margin-left: -1px;
    *zoom: 1;
    margin-bottom: 1px;
}

input[type="text"]:focus,
input[type="text"]:active,
input[type="password"]:focus,
input[type="password"]:active,
textarea:focus,
textarea:active,
textarea.error {
    border: 2px solid #a4a4a4;
    margin-top: -1px;
    margin-left: -1px;
    margin-bottom: 0;
}

input.error,
select.error,
textarea.error {
    border: 2px solid #ffcd01 !important;
}

div.error {
    border: 2px solid #ffcd01 !important;
    height: 26px;
}

textarea {
    padding: 5px 0 5px 5px;
    height: 82px;
    width: 353px;
    overflow: auto;
    resize: none;
}

label {
    font: 14px/18px 'arial';
    color: #000;
}

input[type="submit"] {
    background: url("../images/btn-submit.png") no-repeat;
    padding: 0 0 5px;
    width: 169px;
    height: 54px;
    display: block;
    text-align: center;
    border: none;
    font: bold 14px 'arial';
    color: #fff;
    text-shadow: 0 1px 0 #ff6c00;
    -moz-text-shadow: 0 1px 0 #ff6c00;
    -webkit-text-shadow: 0 1px 0 #ff6c00;
    vertical-align: top;
    cursor: pointer;
}

input[type="submit"]:hover {
    opacity: .85;
}

input[type="submit"]:active,
input[type="submit"]:focus {
    padding: 0;
    background-position: 0 3px;
}

/* form el-pos */
.el-pos {

}

.el-pos li {
    padding: 0 0 15px 165px;
    *overflow: visible;
    *height: 1%;
    background: none;
}

.el-pos li:after {
    content: '';
    clear: both;
    display: block;
}

.el-pos li label {
    float: left;
    margin: 5px 0 0 -165px;
    width: 155px;
}

.el-pos li input[type="submit"] {
    margin: 24px 0 26px -12px;
}

/* btn */
.btn-order {
    display: inline-block;
    *display: inline;
    *margin-left: -1px;
    *zoom: 1;
    background: url("../images/btn-order.png") 100% 0 no-repeat;
    height: 70px;
    font-size: 14px;
    font-weight: bold;
    line-height: 60px;
    color: #fff;
    text-decoration: none;
    text-shadow: 0 1px 0 #cccc00;
    -moz-text-shadow: 0 1px 0 #cccc00;
    -webkit-text-shadow: 0 1px 0 #cccc00;
/*
    text-shadow: 0 1px 0 #ff6c00;
    -moz-text-shadow: 0 1px 0 #ff6c00;
    -webkit-text-shadow: 0 1px 0 #ff6c00;
*/
}

.btn-order span.decor {
    display: block;
    background: url("../images/btn-order.png") 0 -70px no-repeat;
    height: 70px;
    min-width: 162px;
    padding: 0 25px;
    text-align: center;
}

.btn-order:hover {
    background-position: 100% -140px;
}

.btn-order:hover span.decor {
    background-position: 0 -210px;
}

.btn-order:focus,
.btn-order:active {
    background-position: 100% -280px;
    line-height: 73px;
}

.btn-order:focus span.decor,
.btn-order:active span.decor {
    background-position: 0 -350px;
}

/* wrapper */
.wrapper {
    width: 966px;
    margin: 0 auto;
    background: #f8f8f8;
    min-height: 100%;
}

/* header */
