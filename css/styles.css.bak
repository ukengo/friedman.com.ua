}

/* .flexslider */
.flexslider {
    position: relative;
    width: 638px;
    height: 306px;
    opacity: 0;
    overflow: hidden;
}

.flexslider .slides li {
    position: relative;
}

.flexslider .slides li .title {
    background: url("../images/slider-bg3.png");
    width: 100%;
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    font-size: 12px;
    color: #000;
}

.flexslider .slides li .title > span {
    padding: 15px 22px 22px;
    display: block;
}

.flexslider .slides li .title a {
    color: #005b7f;
}

.flexslider .slides li .title .head {
    display: block;
    font: 500 24px 'ubuntu';
    padding: 0 0 8px;
}

.flexslider .flex-control-paging {
    position: absolute;
    top: 9px;
    right: 9px;
    list-style: none;
    z-index: 10;
}

.flexslider .flex-control-paging li {
    padding: 0 0 10px;
}

.flexslider .flex-control-paging li a {
    display: block;
    width: 8px;
    height: 8px;
    text-indent: -999px;
    overflow: hidden;
    border: 2px solid #bf122e;
    background: #bf122e;
    cursor: pointer;
}

.flexslider .flex-control-paging li a:hover,
.flexslider .flex-control-paging li a.flex-active {
    background: url("../images/flexslider-nav.png")
}

/* content-position */
.content-position {
