    top: 16px;
    left: -5px;
}

/* slogan */
.header .slogan {
    position: absolute;
    top: 47px;
    left: 287px;
    color: #959595;
    font: 300 italic 18px/18px 'ubuntu';
}

/* contacts */
.header .contacts {
