}

.main-nav ul li.active a {
    background: url("../images/main-nav-a-sep.gif") 50% 100% no-repeat;
}

/* index-top-sidebar */
.index-top-sidebar {
    padding: 0 328px 18px 0;
    *overflow: visible;
    *height: 1%;
}

.index-top-sidebar:after {
    content: '';
    clear: both;
    display: block;
}

.index-top-sidebar .left-column {
    float: left;
    width: 100%;
}

.index-top-sidebar .right-column {
    float: right;
    margin: 0 -328px 0 0;
