}

/* main-nav */
.main-nav {
    width: 100%;
    overflow: hidden;
}

.main-nav ul {
    padding: 0;
}

.main-nav ul li {
    padding: 0 0 0 1px;
    background: url("../images/main-nav-sep.gif") 0 11px no-repeat;
    height: 45px;
    line-height: 45px;
    font-size: 13px;
    color: #fff;
    float: left;
}

.main-nav ul li.first {
